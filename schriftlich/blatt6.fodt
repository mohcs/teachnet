             <mn>10</mn>
             <mrow>
              <mo stretchy="false">−</mo>
              <mn>5</mn>
             </mrow>
            </msup>
           </mrow>
          </mfrac>
          <mo stretchy="false">∗</mo>
          <mn>4</mn>
         </mrow>
         <mrow>
          <mi mathvariant="italic">ms</mi>
          <mo stretchy="false">=</mo>
          <mn>400000</mn>
         </mrow>
         <mi mathvariant="italic">ms</mi>
        </mrow>
        <annotation encoding="StarMath 5.0">{1} over {10^-5} * 4 ms = 400000 ms</annotation>
       </semantics>
      </math>
